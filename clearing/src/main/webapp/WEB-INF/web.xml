<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<servlet>
		<servlet-name>joinMember</servlet-name>
		<display-name>joinMember</display-name>
		<description></description>
		<servlet-class>com.clearing.controller.joinMember</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>loginChk</servlet-name>
		<display-name>loginChk</display-name>
		<description></description>
		<servlet-class>com.clearing.controller.loginChk</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>logOut</servlet-name>
		<display-name>logOut</display-name>
		<description></description>
		<servlet-class>com.clearing.controller.logOut</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>myPageUpdate</servlet-name>
		<display-name>myPageUpdate</display-name>
		<description></description>
		<servlet-class>com.clearing.controller.myPageUpdate</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>joinMember</servlet-name>
		<url-pattern>/joinMember</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>loginChk</servlet-name>
		<url-pattern>/loginChk</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>logOut</servlet-name>
		<url-pattern>/logOut</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>myPageUpdate</servlet-name>
		<url-pattern>/myPageUpdate</url-pattern>
	</servlet-mapping>

</web-app>
